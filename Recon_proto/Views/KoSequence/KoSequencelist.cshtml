	function map() {
		try {
			$("#check1ko").prop('checked', false);
			var grid = $("#grid_ruleversion").data("kendoGrid");
			var grid1 = $("#themerecon_grid").data("kendoGrid");
			var grid2 = $("#process_grid").data("kendoGrid");
			var itemGrid = $("#grid_KoSequence").data("kendoGrid");
			var selected = [];
			grid.tbody.find("input:checked").closest("tr").each(function (index) {
				selected.push(grid.dataItem(this));
			});
			grid1.tbody.find("input:checked").closest("tr").each(function (index) {
				selected.push(grid1.dataItem(this));
			});
			grid2.tbody.find("input:checked").closest("tr").each(function (index) {
				selected.push(grid2.dataItem(this));
			});
			var result = selected;
			if (result.length == 0) {
				kendoAlert("Please select records");
			}
			else {
				var result = selected;
				var i = 0;
				var max = 0;
				for (var j = 0; j < itemGrid.dataSource._data.length; j++) {
					if (itemGrid.dataSource._data[j].ko_seqno > max) {
						max = itemGrid.dataSource._data[j].ko_seqno;
					}
				}
				var i = parseInt(max);

				$.each(result, function (index, value) {
					var isRecordAvl = false;
					i = i + 1;
					if (!isRecordAvl) {
						if(value.ko_type == "R") {
							var ds = {ko_seqno:i,serial_no:i, ko_seq_type_desc: value.ko_type_desc, ko_seq_type: value.ko_type, ko_seq_name: value.rule_name, ko_seq_ref_code: value.rule_code,hold_flag_desc: ""};
							itemGrid.dataSource.add(ds);
						} else if(value.ko_type == "T") {
							var ds = {ko_seqno:i,serial_no:i, ko_seq_type_desc: value.ko_type_desc, ko_seq_type: value.ko_type, ko_seq_name: value.theme_desc, ko_seq_ref_code: value.theme_code,hold_flag_desc: ""};
							itemGrid.dataSource.add(ds);
						} else if(value.ko_type == "P") {
							var ds = {ko_seqno:i,serial_no:i, ko_seq_type_desc: value.ko_type_desc, ko_seq_type: value.ko_type, ko_seq_name: value.preprocess_desc, ko_seq_ref_code: value.preprocess_code,hold_flag_desc: "" };
							itemGrid.dataSource.add(ds);
						}
					}
				});
				itemGrid.refresh();
			}
			$('#selectall').prop('checked', false);
			$("#check2ko").prop('checked', true);
		}
		catch (err) {
			javascript_log4j_root(arguments.callee.name, err);
		}
	}
	function save() {
		var in_Koseq_gid = 0;
		var in_recon_code = $("#cmbreconname").val();
		var in_recon_version = $("#comreconversion").val();
		var in_recon_versionold = $("#oldcomreconversion").val();
			if (in_recon_version == in_recon_versionold) {
				$.alert({
					title: 'Recon',
					content: "Need to Change Recon Verion",
					type: 'green',
				});
				return;
			}
		var in_action = "INSERT"
		var action_by = sessionStorage.getItem("usrname");
		var concatstring2 = "";
		var grid_val = JSON.stringify($("#grid_KoSequence").data().kendoGrid._data);
		var flag = "";
		var Bind = {};
		Bind = grid_val;
		var result = JSON.parse(Bind);
		$.each(result, function (index, value) {
			var row_id = value.ko_seqno;
			var ko_seq_ref_code = value.ko_seq_ref_code;
			var ko_seq_type =value.ko_seq_type;
			var in_koseq_hold = "";
			if ($('#chk_sel1_' + value.serial_no).is(":checked")) {
				in_koseq_hold = "Y";
			} else {
				in_koseq_hold = "N";
			}
			if (concatstring2 != "") {
				flag = "$"
			}
			concatstring2 = concatstring2 + flag + ko_seq_ref_code + "#" + row_id + "#" + in_koseq_hold+ "#" + ko_seq_type
		});
		$.ajax({
			type: "POST",
			url: '../KoSequence/koseqsave',
			dataType: 'json',
			data: JSON.stringify({
				in_Koseq_gid: in_Koseq_gid, in_recon_code: in_recon_code, in_recon_version:in_recon_version,in_Koseq_recon: concatstring2, in_action_by: action_by,in_action: in_action
			}),
			contentType: 'application/json; charset=utf-8',
			success: function (response) {
				var res = response;
				if (response != null) {
					$.alert({
						title: 'Recon',
						content: res.out_msg,
						type: 'green',
					});
				}
				else {
					$.alert({
						title: 'Recon',
						content: res.out_msg,
						type: 'green',
					});
				}
			},
			error: function (er) {
				alert(er)
				console.log(er)
			}
		});
	}
	function dublicates() {
		var grid = $("#grid_KoSequence").data("kendoGrid");
		var data = grid.dataSource.data();

		var seqMap = {};
		var duplicates = [];

		data.forEach(function (item) {
			var val = item.ko_seqno;
			if (val !== undefined && val !== null) {
				if (seqMap[val]) {
					// Found a duplicate
					if (!duplicates.includes(val)) {
						duplicates.push(val);
					}
				} else {
					seqMap[val] = true;
				}
			}
		});

		if (duplicates.length > 0) {
			$.alert({
				title: 'Recon',
				content: "Duplicate seq_no values: " + duplicates.join(", "),
				type: 'green',
			});
		} else {
			save();
		}
	}
	function myDeleteJsko() {
		var grid = $("#grid_KoSequence").data("kendoGrid");
		var model = grid.dataItem($(event.target).closest("tr"));
		grid.dataSource.remove(model);
	}
</script>
