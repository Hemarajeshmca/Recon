@{
	ViewBag.title = "Ko Sequence";
	Layout = "~/Views/Shared/_LayoutNew.cshtml";
}
<!Doctype html>
<html>
<head>
	<title></title>
	<style>
		.nav-item.me-2.me-xl-50 {
			margin-right: 33rem !important;
		}

		.k-grid-header th.k-header > .k-link {
			color: white !important;
		}

		.error_show {
			color: red;
			margin-left: 10px;
		}

		.k-grid.k-widget {
			height: 360px;
		}

		table {
			border: 1px solid #C5C5C5 !important;
		}

		.k-grid-content {
			overflow-x: hidden;
			/* height: 380px !important; */
		}

		.k-grid-header {
			font-weight: bold;
		}

		.k-content {
			font-weight: normal;
			font-size: 13px;
		}

		.k-widget.k-window {
			padding-top: 35px;
			min-width: 90px;
			min-height: 50px;
			width: 700px !important;
			height: 300px !important;
			top: 180.766px !important;
			/*left: 120px !important;*/
			z-index: 10003;
			left: 300px !important;
		}


		.accordion-wrapper1 {
			border-radius: 0.5rem !important;
			overflow: hidden;
			box-shadow: 0 4px 4px -2px rgba(0, 0, 0, 0.5);
			width: 100%;
			margin: 0 auto;
		}

		.accordion {
			width: 100%;
			color: white;
			overflow: hidden;
			margin-bottom: 9px;
		}

			.accordion:last-child {
				margin-bottom: 0;
			}

		.accordion-label {
			display: flex;
			-webkit-box-pack: justify;
			justify-content: space-between;
			padding: 4px;
			background: rgb(135, 46, 123);
			/*background: rgba(4,57,94,.8);*/
			font-weight: bold;
			cursor: pointer;
			font-size: 14px;
			color: white;
		}

			.accordion-label:hover {
				background: rgb(135, 46, 123)
			}

			.accordion-label::after {
				content: "\276F";
				width: 16px;
				height: 16px;
				text-align: center;
				-webkit-transition: all 0.3s;
				transition: all 0.3s;
			}

		.accordion-content {
			max-height: 0;
			overflow: hidden;
			/* padding: 0 16px; */
			transition: max-height 0.3s ease;
		}

			.accordion-content p {
				margin: 0;
				color: rgba(4,57,94,.7);
				font-size: 18px;
			}

		input:checked ~ .accordion-content {
			max-height: 100vh;
			/* padding: 16px; */
		}

		.accordion-body {
			padding: 0.09rem 0.125rem !important;
		}

	</style>
</head>
<body class="hold-transition skin-blue sidebar-mini fixed">
	<div class="content-wrapper">
		<div class="container-xxl flex-grow-1 container-p-y">
			<div class="card-body" style="margin-top: -1%; min-height: 82%;">
				<div class="row" style="margin-top:-19px">
					<div class="col-sm-3" style="margin-bottom:5px;">
						<label for="" class="form-label">Recon Name&nbsp;<span style="font-size: 12px;color: red;font-weight: bold;">*</span></label>
						<select id="cmbreconname" class="form-select form-control" onchange="reconchangelist()">
						</select>
					</div>
					<div class="col-sm-3">
						<label for="" class="form-label">Recon Version&nbsp;<span style="font-size: 12px;color: red;font-weight: bold;">*</span></label>
						<input class="form-control" type="text" id="comreconversion">
						<input class="form-control" type="hidden" id="oldcomreconversion">
					</div>
					<div class="col-sm-3">
						<label for="" class="form-label">Processing Method&nbsp;<span style="font-size: 12px;color: red;font-weight: bold;">*</span></label>
						<input class="form-control" type="text" id="promethod" disabled>
						<input class="form-control" type="hidden" id="promethodcode" disabled>						
					</div>
					<div class="col-sm-3">
						<button type="button" id="promap" class="btn btn-sm save_btn me-2" style="margin-top:30px" onclick="map()">Process Map</button>&nbsp;&nbsp;&nbsp;
						<button type="button" id="" class="btn btn-sm save_btn me-2" style="margin-top:30px" onclick="dublicates()">Save</button>&nbsp;&nbsp;&nbsp;
					</div>
				</div>
				<div class="row">
					<div class="accordion-wrapper1" style="padding-bottom: 10px;padding-top: 10px;">
						<div class="accordion" id="headingsequence">
							<input type="checkbox" name="radio-a" id="check1ko" style="display:none">
							<label class="accordion-label" for="check1ko">&nbsp;&nbsp;&nbsp;Rule / Process / Theme List</label>
							<div id="headingsequence" class="accordion-content">
								<div class="accordion-body" style="padding-left:15px;">
									<div class="nav-align-top mt-3">
										<ul class="nav nav-pills mb-2" role="tablist">
											<li class="nav-item">
												<button type="button" id="preprocesstab"
														class="nav-link"
														role="tab"
														data-bs-toggle="tab" onclick="preprocessversiontab()"
														aria-controls="navs-pills-top-preprocess"
														data-bs-target="#navs-pills-top-preprocess">
													Preprocess
												</button>
											</li>
											<li class="nav-item">
												<button type="button" id="ruletab"
														class="nav-link active"
														role="tab"
														data-bs-toggle="tab" onclick="ruleversiontab()"
														aria-controls="navs-pills-top-Rule"
														data-bs-target="#navs-pills-top-Rule">
													Rule
												</button>
											</li>
											<li class="nav-item">
												<button type="button" id="themetab"
														class="nav-link"
														role="tab"
														data-bs-toggle="tab" onclick="themeversiontab()"
														aria-controls="navs-pills-top-Theme"
														data-bs-target="#navs-pills-top-Theme">
													Theme
												</button>
											</li>
											<li class="nav-item">
												<button type="button" id="postprocesstab"
														class="nav-link"
														role="tab"
														data-bs-toggle="tab" onclick="postprocessversiontab()"
														aria-controls="navs-pills-top-postprocess"
														data-bs-target="#navs-pills-top-postprocess">
													Postprocess
												</button>
											</li>
											<li class="nav-item">
												<button type="button" id="processtab"
														class="nav-link"
														role="tab"
														data-bs-toggle="tab" onclick="processversiontab()"
														aria-controls="navs-pills-top-process"
														data-bs-target="#navs-pills-top-process">
													Process
												</button>
											</li>
										</ul>
										<div class="tab-content" style="box-shadow:none !important;padding: 20px 0px;">
											<div class="tab-pane fade" id="navs-pills-top-preprocess" role="tabpanel" style="margin-top:-25px;">
												<div class="panel panel-default">
													<div class="panel-body">
														<div class="col-xs-12">
															<div id="preprocess_grid">
															</div>
														</div>
													</div>
												</div>
											</div>
											<div class="tab-pane fade" id="navs-pills-top-Rule" role="tabpanel" style="margin-top:-25px;">
												<div class="panel panel-default">
													<div class="panel-body">
														<div class="col-xs-12">
															<div id="grid_ruleversion">
															</div>
														</div>
													</div>
												</div>
											</div>
											<div class="tab-pane fade" id="navs-pills-top-Theme" role="tabpanel" style="margin-top:-25px;">
												<div class="panel panel-default">
													<div class="panel-body">
														<div class="col-xs-12">
															<div id="themerecon_grid"> </div>
														</div>
													</div>
												</div>
											</div>
											<div class="tab-pane fade" id="navs-pills-top-postprocess" role="tabpanel" style="margin-top:-25px;">
												<div class="panel panel-default">
													<div class="panel-body">
														<div class="col-xs-12">
															<div id="postprocess_grid"> </div>
														</div>
													</div>
												</div>
											</div>
											<div class="tab-pane fade" id="navs-pills-top-process" role="tabpanel" style="margin-top:-25px;">
												<div class="panel panel-default">
													<div class="panel-body">
														<div class="col-xs-12">
															<div id="process_grid"> </div>
														</div>
													</div>
												</div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
						<div class="accordion" id="headingprocess" onclick="headingsequence()">
							<input type="checkbox" name="radio-a" id="check2ko" style="display:none">
							<label class="accordion-label" for="check2ko">&nbsp;&nbsp;&nbsp;Processing Sequence</label>
							<div id="headingprocess" class="accordion-content" style="padding-top:1px;">
								<div class="accordion-body" style="padding-left:1px;">
									<div class="col-xs-12">
										<div id="gd_KoSequence">
											<div id="grid_KoSequence"> </div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="modal fade" id="ReconRuledetail" tabindex="-1" aria-hidden="true" data-bs-backdrop="static">
			<div class="modal-dialog modal-xl">
				<div class="modal-content" style="height:585px;">
					<div class="modal-header">
					</div>
					<div class="modal-body" style="margin-top: -26px;">
						<div class="row">
							<div class="col-sm-4">
							</div>
							<div class="col-sm-4">
								<h4 style="font-size: 15px;text-align: center;color: #1a9950;font-weight: 600;">Rule Detail</h4>
							</div>
							<div class="col-sm-4">
								<i class="bx bx-x cursor-pointer" data-bs-dismiss="modal" style="color: #000000;float:right"></i>
							</div>
						</div>
						<hr style="margin-top:-1px; width:100%;">
						<div class="container" style="padding:0px !important;">
							@await Html.PartialAsync("_koRuleviewdetail")
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="modal fade" id="Reconthemedetail" tabindex="-1" aria-hidden="true" data-bs-backdrop="static">
			<div class="modal-dialog modal-xl">
				<div class="modal-content" style="height:549px;">
					<div class="modal-header">
					</div>
					<div class="modal-body" style="margin-top: -26px;">
						<div class="row">
							<div class="col-sm-4">
							</div>
							<div class="col-sm-4">
								<h4 style="font-size: 15px;text-align: center;color: #1a9950;font-weight: 600;">Theme Detail</h4>
							</div>
							<div class="col-sm-4">
								<i class="bx bx-x cursor-pointer" data-bs-dismiss="modal" style="color: #000000;float:right"></i>
							</div>
						</div>
						<hr style="margin-top:-1px; width:100%;">
						<div class="container" style="padding:0px !important;">
							@await Html.PartialAsync("_kothemeviewdetail")
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="modal" id="Reconpreprocessdetail" tabindex="-1" aria-hidden="true" data-bs-backdrop="static">
			<div class="modal-dialog modal-xl">
				<div class="modal-content" style="height:550px;">
					<div class="modal-header">
					</div>
					<div class="modal-body" style="margin-top: -26px;">
						<div class="row">
							<div class="col-sm-4">
							</div>
							<div class="col-sm-4">
								<h4 style="font-size: 15px;text-align: center;color: #1a9950;font-weight: 600;">Preprocess Detail</h4>
							</div>
							<div class="col-sm-4">
								<i class="bx bx-x cursor-pointer" data-bs-dismiss="modal" style="color: #000000;float:right"></i>
							</div>
						</div>
						<hr style="margin-top:-1px; width:100%;">
						<div class="container" style="padding:0px !important;">
							@await Html.PartialAsync("_kopreprocessdetail")
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</body>
</html>
<script>
	$(document).ready(function () {
		$("#navhead").text("Recon Processing Sequence");
		grid_ruleversion([]);
		theme_grid([]);
		grid_process([]);
		gridlist();
		$("#check1ko").prop('checked', true);
		grid_KoSequence_details([]);		
	});
	function ruleversiontab() {
		if ($('#ruletab').attr('class') != 'nav-link active') {
			$("#ruletab").removeClass('nav-link');
			$("#ruletab").toggleClass('nav-link active');		
			$("#preprocesstab").toggleClass('nav-link');
			$("#preprocesstab").removeClass('nav-link active');
		}
		$("div#navs-pills-top-postprocess").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-postprocess").toggleClass('tab-pane fade');
		$("div#navs-pills-top-preprocess").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-preprocess").toggleClass('tab-pane fade');
		$("div#navs-pills-top-Theme").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-process").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-Theme").toggleClass('tab-pane fade');
		$("div#navs-pills-top-process").toggleClass('tab-pane fade');
		$("div#navs-pills-top-Rule").toggleClass('tab-pane fade');
		$("div#navs-pills-top-Rule").toggleClass('tab-pane fade active show');
		$("#grid_ruleversion").data("kendoGrid")?.resize();
	}
	function themeversiontab() {
		$("div#navs-pills-top-postprocess").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-postprocess").toggleClass('tab-pane fade');
		$("div#navs-pills-top-preprocess").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-preprocess").toggleClass('tab-pane fade');
		$("div#navs-pills-top-Rule").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-process").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-Rule").toggleClass('tab-pane fade');
		$("div#navs-pills-top-process").toggleClass('tab-pane fade');
		$("div#navs-pills-top-Theme").removeClass('tab-pane fade');
		$("div#navs-pills-top-Theme").toggleClass('tab-pane fade active show');
		$("#themerecon_grid").data("kendoGrid")?.resize();
	}
	function processversiontab() {
		$("div#navs-pills-top-Rule").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-Rule").toggleClass('tab-pane fade');
		$("div#navs-pills-top-postprocess").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-postprocess").toggleClass('tab-pane fade');
		$("div#navs-pills-top-preprocess").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-preprocess").toggleClass('tab-pane fade');
		$("div#navs-pills-top-Theme").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-Theme").toggleClass('tab-pane fade');
		$("div#navs-pills-top-process").removeClass('tab-pane fade');
		$("div#navs-pills-top-process").toggleClass('tab-pane fade active show');
		$("#process_grid").data("kendoGrid")?.resize();
	}
	function preprocessversiontab() {
		if ($('#preprocesstab').attr('class') != 'nav-link active') {
			$("#ruletab").removeClass('nav-link active');
			$("#ruletab").toggleClass('nav-link');
			$("#preprocesstab").removeClass('nav-link');
			$("#preprocesstab").toggleClass('nav-link active');
		}
		$("div#navs-pills-top-Rule").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-Rule").toggleClass('tab-pane fade');
		$("div#navs-pills-top-postprocess").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-postprocess").toggleClass('tab-pane fade');
		$("div#navs-pills-top-Theme").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-process").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-Theme").toggleClass('tab-pane fade');
		$("div#navs-pills-top-process").toggleClass('tab-pane fade');
		$("div#navs-pills-top-preprocess").removeClass('tab-pane fade');
		$("div#navs-pills-top-preprocess").toggleClass('tab-pane fade active show');
		$("#preprocess_grid").data("kendoGrid")?.resize();
	}
	function postprocessversiontab() {
		$("div#navs-pills-top-preprocess").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-preprocess").toggleClass('tab-pane fade');
		$("div#navs-pills-top-Rule").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-Rule").toggleClass('tab-pane fade');
		$("div#navs-pills-top-Theme").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-process").removeClass('tab-pane fade active show');
		$("div#navs-pills-top-Theme").toggleClass('tab-pane fade');
		$("div#navs-pills-top-process").toggleClass('tab-pane fade');
		$("div#navs-pills-top-postprocess").removeClass('tab-pane fade');
		$("div#navs-pills-top-postprocess").toggleClass('tab-pane fade active show');
		$("#postprocess_grid").data("kendoGrid")?.resize();
	}
	function headingsequence () {
		$("#check1ko").prop('checked', false);
	}
	function reconchangelist () {
		grid_ruleversion([]);
		theme_grid([]);
		grid_process([]);
		if ($("#cmbreconname").val() != '') {
			sessionStorage.setItem("reconcode", $("#cmbreconname").val());
			sessionStorage.setItem("reconname", $("#cmbreconname :selected").text());
			setcon();
		}
		var in_recon_code = $("#cmbreconname").val();
		var in_user_code = sessionStorage.getItem("usrname");
		$.ajax({
			type: "POST",
			url: '../KoSequence/ReconkoseqListfetch',
			dataType: 'json',
			async: false,
			data: JSON.stringify({ in_recon_code: in_recon_code,in_user_code: in_user_code }),
			contentType: 'application/json; charset=utf-8',
			success: function (response) {
				if (response != null) {
					var res = JSON.parse(response);
					$("#comreconversion").val(res.Reconversion[0].recon_rule_version);
					$("#oldcomreconversion").val(res.Reconversion[0].recon_rule_version);
					$("#promethod").val(res.Reconversion[0].processing_method);
					$("#promethodcode").val(res.Reconversion[0].processing_method_code);
					if(res.Reconversion[0].processing_method_code == "S"){
						$("#processtab").hide();
						preprocessversiontab();
						$("#preprocesstab").show();
						$("#postprocesstab").show();
						$("#promap").hide();
						$("#checkAllpostprocess").hide();
						$("#checkAllpreprocess").hide();
						$("#checkAllprocess").hide();
						$("#checkAlltheme").hide();
						$("#checkAll").hide();
					}else{
						ruleversiontab();
						$("#preprocesstab").hide();
						$("#postprocesstab").hide();
						$("#processtab").show();
						$("#promap").show();
						$("#checkAllpostprocess").show();
						$("#checkAllpreprocess").show();
						$("#checkAllprocess").show();
						$("#checkAlltheme").show();
						$("#checkAll").show();
					}
					if (res.versionlist.length > 0) {
						grid_ruleversion(res.versionlist);

					} else {
						grid_ruleversion([]);
					}
					if (res.theme.length > 0) {
						theme_grid(res.theme);
					} else {
						theme_grid([]);
					}
					if (res.process.length > 0) {
						grid_process(res.process);
					} else {
						grid_process([]);
					}
					if (res.preprocess.length > 0) {
						grid_preprocess(res.preprocess);
					} else {
						grid_preprocess([]);
					}
					if (res.postprocess.length > 0) {
						grid_postprocess(res.postprocess);
					} else {
						grid_postprocess([]);
					}
				}
			},
			error: function (er) {
				alert(er)
				console.log(er)
			}
		});
		var in_recon_code = $("#cmbreconname").val();
		var in_user_code = sessionStorage.getItem("usrname");
		$.ajax({
			type: "POST",
			url: '../KoSequence/seqlistfetch',
			dataType: 'json',
			async: false,
			data: JSON.stringify({ in_recon_code: in_recon_code,in_user_code: in_user_code }),
			contentType: 'application/json; charset=utf-8',
			success: function (response) {
				if (response != null) {
					var res = JSON.parse(response);
					if (res.length > 0) {
						grid_KoSequence_details(res);
						$("#grid_KoSequence").data("kendoGrid")?.resize();
					} else {
						grid_KoSequence_details([]);
					}
				}
			}
		});
	}
	function gridlist() {
		var data = {};
		data.in_user_gid = 0;
		data.in_active_status = '';
		data.in_user_code = sessionStorage.getItem("usrname");
		var Context = data;
		$.ajax({
			type: "POST",
			url: '../Recon/Reconlistfetch',
			dataType: 'json',
			data: JSON.stringify(data),
			contentType: 'application/json; charset=utf-8',
			success: function (response) {
				var res = JSON.parse(response);
				$("#cmbreconname").empty();
				$("#cmbreconname").append($('<option>', {
					value: "",
					text: "  Select  "
				}));
				$.each(res, function (index, item) {
					if (item.active_status == "Y") {
						$("#cmbreconname").append($('<option>', {
							value: item.recon_code,
							text: item.recon_name
						}));
					}
				});
				if (sessionStorage.getItem("reconcode") != null) {
					$("#cmbreconname").val(sessionStorage.getItem("reconcode"));
					jQuery('#cmbreconname').trigger("change");
				}
			},
			error: function (er) {
				alert(er)
				console.log(er)
			}
