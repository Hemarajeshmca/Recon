	function gridlist() {
		if ($("#cmbreconname").val() != '') {
			sessionStorage.setItem("reconcode", $("#cmbreconname").val());
			sessionStorage.setItem("reconname", $("#cmbreconname :selected").text());
			setcon();
		}
		var data = {};
		data.recon_code = $("#cmbreconname").val();
		data.in_user_code = sessionStorage.getItem("usrname");
		if (data.recon_code) {
			$.ajax({
				type: "POST",
				url: '../Theme/Themelistfetch',
				dataType: 'json',
				contentType: 'application/json; charset=utf-8',
				data: JSON.stringify(data),
				success: function (response) {
					if (response != " " && response != null && response != "{}" && response != "[]") {
						var res = JSON.parse(response)
						$("#gd_themelist").empty();
						$("#gd_themelist").append("<div id='grid_themelist'></div>");
						var activeItems = res.filter(item => item.active_status == 'Y');
						grid_themelist(activeItems);
					}
					else {
						grid_themelist([]);
					}
				},
				error: function (er) {
					alert(er)
					console.log(er)
				}
			});
		} else {
			grid_themelist([]);
		}
	}

	function grid_themelist(data) {
		try {
			$("#grid_themelist").kendoGrid({
				dataSource: {
					data: data,
					pageSize: 15,
					schema: {
						model: {
							fields: {
								theme_gid: { type: "string" },
								theme_code: { type: "string" },
								theme_desc: { type: "string" },
								theme_order: { type: "number" },
								recon_code: { type: "string" },
								recon_name: { type: "string" },
								active_status: { type: "string" },
