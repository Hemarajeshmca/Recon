	function showDatacountGrid(dataset_code) {
		$.ajax({
			type: "POST",
			url: '../KnockOff/recondatasetinfo',
			dataType: 'json',
			async: false,
			data: JSON.stringify({ in_recon_code: $("#reconname").val(), in_dataset_code: dataset_code, in_automatch_flag: 'N' }),
			contentType: 'application/json; charset=utf-8',
			success: function (response) {
				if (response) {
					var res = JSON.parse(response)
					if (res.Table.length > 0) {
						loadGrid(res.Table);
					} else {
						loadGrid([]);
						$.alert({
							title: 'Recon',
							content: response,
							type: 'red',
						});
					}
				} else {
					$.alert({
						title: 'Recon',
						content: "Something Went Wrong..!",
						type: 'red',
					});
				}

			},
			error: function (er) {
				alert(er)
				console.log(er)
			}
		});
	}

